{
  "name": "nextjs-webrtc-firebase",
  "description": "Next.js ＆ Firebase の学習用サンプルアプリケーションです。",
  "version": "1.0.0",
  "scripts": {
    "dev": "NODE_ENV=development next",
    "start": "NODE_ENV=production next start",
    "type-check": "tsc",
    "lint": "eslint --ext .ts,.tsx .",
    "lintfix": "eslint --fix --ext .ts,.tsx .",
    "format": "prettier -w 'src/**/*.{ts,tsx}'",
    "fix": "yarn format && yarn lint",
    "test": "jest",
    "test:coverage": "jest --collect-coverage",
    "build": "NODE_ENV=production USE_FIREBASE_EMULATOR=false next build && next export",
    "deploy:production": "firebase deploy",
    "deploy:preview": "firebase hosting:channel:deploy preview_name",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,less,sass,scss}": [
      "stylelint --fix"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
